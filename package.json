{
    "name": "batch-replace",
    "displayName": "batch replace",
    "description": "Batch Replace",
    "keywords": [
        "vscode",
        "plugin",
        "batchreplace"
    ],
    "version": "1.0.3",
    "publisher": "raindrop",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "assets/images/icon.png",
    "activationEvents": [
        "*"
    ],
    "main": "./src/extension",
    "contributes": {
        "configuration": {
        },
        "commands": [
            {
                "command": "extension.batchReplace",
                "title": "Batch Replace"
            }
        ],
        "keybindings": [
            {
                "command": "extension.batchReplace",
                "key": "ctrl+f9",
                "mac": "cmd+f9",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.batchReplace",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "extension.batchReplace",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "bugs": {
        "url": "https://github.com/rainpure/batch-replace/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rainpure/batch-replace"
    },
    "homepage": "https://github.com/rainpure/batch-replace/blob/master/README.md"
}
